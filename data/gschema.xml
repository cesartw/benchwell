<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/io/benchwell/" id="io.benchwell">
		<key name="window-pos-x" type="i">
			<default>360</default>
			<summary>Window horizontal position</summary>
			<description>The saved window horizontal position</description>
		</key>
		<key name="window-pos-y" type="i">
			<default>360</default>
			<summary>Window vertical position</summary>
			<description>The saved window vertical position</description>
		</key>
		<key name="window-size-w" type="i">
			<default>1432</default>
			<summary>Window width</summary>
			<description>The saved window width</description>
		</key>
		<key name="window-size-h" type="i">
			<default>867</default>
			<summary>Window height</summary>
			<description>The saved window height</description>
		</key>
		<key name="tab-position" type="s">
			<default>"TOP"</default>
			<summary>Tab position</summary>
			<description>The tool tab saved position</description>
		</key>
		<key name="environment-id" type="x">
			<default>0</default>
			<summary>Environment</summary>
			<description>The last environment id selected</description>
		</key>
		<key name="http-collection-id" type="x">
			<default>0</default>
			<summary>HTTP Collection ID</summary>
			<description>The last http collection id selected</description>
		</key>
		<key name="http-item-id" type="x">
			<default>0</default>
			<summary>HTTP Request ID</summary>
			<description>The last http request opened</description>
		</key>
		<key name="http-tree-state" type="s">
			<default>"{}"</default>
			<summary>HTTP tree state</summary>
			<description>Map of items expanded</description>
		</key>
		<key name="http-font" type="s">
			<default>""</default>
			<summary>Http font</summary>
			<description>Http treeview font</description>
		</key>
		<key name="db-query-history-limit" type="i">
			<default>25</default>
			<summary>Database query history</summary>
			<description>Maximum number of queries kept in the history</description>
		</key>
		<key name="db-filters" type="s">
			<default>"{}"</default>
			<summary>Database filters</summary>
			<description>JSON serialization of filters used</description>
		</key>
		<key name="http-single-click-activate" type="b">
			<default>true</default>
			<summary>Request activation</summary>
			<description>Single click activates request</description>
		</key>
		<key name="dark-mode" type="b">
			<default>true</default>
			<summary>Dark Mode</summary>
			<description>Whether to use the dark mode version of the current theme</description>
		</key>
		<key name="editor-theme" type="s">
			<default>""</default>
			<summary>Editor theme</summary>
			<description>Text editor color theme</description>
		</key>
		<key name="editor-font" type="s">
			<default>""</default>
			<summary>Editor font</summary>
			<description>Text editor font</description>
		</key>
		<key name="editor-tab-width" type="i">
			<default>4</default>
			<summary>Tab width</summary>
			<description>Text editor tab width</description>
		</key>
		<key name="editor-line-number" type="b">
			<default>false</default>
			<summary>Line numbers</summary>
			<description>Show line numbers</description>
		</key>
		<key name="editor-highlight-line" type="b">
			<default>false</default>
			<summary>Highlight line</summary>
			<description>Highline current line</description>
		</key>
		<key name="editor-no-tabs" type="b">
			<default>false</default>
			<summary>Do not insert tabs</summary>
			<description>Insert spaces instead of tabs</description>
		</key>
		<key name="pomodoro-duration" type="i">
			<default>1500</default>
			<summary>Work set seconds</summary>
			<description>Pomodoro work set duration in seconds</description>
		</key>
		<key name="pomodoro-break-duration" type="i">
			<default>300</default>
			<summary>Short break duration</summary>
			<description>Pomodoro short break duration in seconds</description>
		</key>
		<key name="pomodoro-long-break-duration" type="i">
			<default>600</default>
			<summary>Long break duration</summary>
			<description>Pomodoro long break duration in seconds</description>
		</key>
		<key name="pomodoro-set-count" type="i">
			<default>4</default>
			<summary>Work set count</summary>
			<description>Pomodoro work set count before a long break</description>
		</key>
	</schema>
</schemalist>
