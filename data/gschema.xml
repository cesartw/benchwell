<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/io/benchwell/" id="io.benchwell">
		<key name="window-pos-x" type="i">
			<default>360</default>
			<summary>Window horizontal position</summary>
			<description>The saved window horizontal position</description>
		</key>
		<key name="window-pos-y" type="i">
			<default>360</default>
			<summary>Window vertical position</summary>
			<description>The saved window vertical position</description>
		</key>
		<key name="window-size-w" type="i">
			<default>1432</default>
			<summary>Window width</summary>
			<description>The saved window width</description>
		</key>
		<key name="window-size-h" type="i">
			<default>867</default>
			<summary>Window height</summary>
			<description>The saved window height</description>
		</key>
		<key name="tab-position" type="s">
			<default>"TOP"</default>
			<summary>Tab position</summary>
			<description>The tool tab saved position</description>
		</key>
		<key name="environment-id" type="x">
			<default>0</default>
			<summary>Environment</summary>
			<description>The last environment id selected</description>
		</key>
		<key name="http-collection-id" type="x">
			<default>0</default>
			<summary>HTTP Collection ID</summary>
			<description>The last http collection id selected</description>
		</key>
		<key name="http-item-id" type="x">
			<default>0</default>
			<summary>HTTP Item</summary>
			<description>The last http item id selected</description>
		</key>
		<key name="db-query-history-limit" type="i">
			<default>25</default>
			<summary>Database query history</summary>
			<description>Maximum number of queries kept in the history</description>
		</key>
		<key name="db-filters" type="s">
			<default>"{}"</default>
			<summary>Database filters</summary>
			<description>JSON serialization of filters used</description>
		</key>
	</schema>
</schemalist>
